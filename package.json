{
  "name": "vue-axios-manager",
  "private": false,
  "version": "2.0.1",
  "type": "module",
  "description": "Vue Axios Manager is a lightweight plugin that enables you to manage multiple Axios endpoints in any Vue application with ease and clarity",
  "license": "MIT",
  "readme": "README.md",
  "source": "src/index.ts",
  "main": "dist/vue-axios-manager.js",
  "module": "dist/vue-axios-manager.mjs",
  "types": "dist/vue-axios-manager.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "TODO.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "Axios",
    "Vue 3",
    "API",
    "Fetch",
    "fetch",
    "http",
    "request",
    "vue"
  ],
  "author": {
    "name": "John Pendenque",
    "email": "pendenquejohn@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zadigo/vue-axios-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Zadigo/vue-axios-manager/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/vue-axios-manager.d.ts",
      "require": "./dist/vue-axios-manager.js",
      "import": "./dist/vue-axios-manager.mjs",
      "default": "./dist/vue-axios-manager.mjs"
    }
  },
  "scripts": {
    "dev": "vite playground",
    "build": "rollup -c rollup.config.mjs",
    "test": "vitest run --coverage",
    "test:wa": "vitest watch --coverage",
    "preview": "vite preview",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "publish": "pnpm run lint && git push && git push --follow-tags && rollup -c && pnpm run docs:build && pnpm publish",
    "publish:dry": "npm pack --dry-run",
    "publish:alpha": "pnpm run lint && git push && git push --follow-tags && rollup -c && pnpm run docs:build && pnpm publish --tag alpha",
    "publish:beta": "pnpm run lint && git push && git push --follow-tags && rollup -c && pnpm run docs:build && pnpm publish --tag beta",
    "publish:rc": "pnpm run lint && git push && git push --follow-tags && rollup -c && pnpm run docs:build && pnpm publish --tag rc",
    "lint": "eslint ."
  },
